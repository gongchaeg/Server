generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id           Int     @id @default(autoincrement())
  nickname     String  @unique @db.VarChar(50)
  profileImage String? @db.VarChar(200)
  intro        String? @db.VarChar(100)
}
